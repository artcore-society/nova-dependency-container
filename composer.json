{
    "name": "artcore-society/nova-dependency-container",
    "description": "nova-dependency-container fork to support Nova 5",
    "keywords": [
        "laravel",
        "nova",
        "nova-4",
        "field"
    ],
    "authors": [
        {
            "name": "alexwenzel",
            "email": "alexander.wenzel.berlin@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Alexwenzel\\DependencyContainer\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Alexwenzel\\DependencyContainer\\FieldServiceProvider"
            ]
        },
        "forked-from": "alexwenzel/nova-dependency-container"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
